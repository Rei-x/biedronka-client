{
  "name": "biedronka-client",
  "version": "1.3.0",
  "description": "Reverse engineered client for mobile Biedronka app",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublish": "pnpm build",
    "docs": "typedoc --out docs src",
    "test:dev": "vitest",
    "test:login": "tsx ./src/tests/login.ts",
    "release": "np --tests false"
  },
  "keywords": [
    "biedronka",
    "reverse-engineering",
    "typescript",
    "playwright",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rei-x/biedronka-client.git"
  },
  "homepage": "https://github.com/Rei-x/biedronka-client",
  "bugs": {
    "url": "https://github.com/Rei-x/biedronka-client/issues"
  },
  "author": "Rei-x",
  "license": "MIT",
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "readline-sync": "^1.4.10",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "playwright": "*",
    "playwright-extra": "*",
    "puppeteer-extra-plugin-stealth": "*"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/readline-sync": "^1.4.8",
    "date-fns": "^3.3.0",
    "dotenv": "^16.4.5",
    "np": "^10.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typedoc": "^0.25.7",
    "vitest": "^1.2.1"
  }
}
